{{#if user.admin}}
<nav style="background-color: #ffe600;" class="navbar navbar-expand-lg">
  <!-- Container wrapper -->
  <div class="container-fluid">
    <!-- Toggle button -->
    <button class="navbar-toggler" type="button" data-mdb-toggle="collapse" data-mdb-target="#navbarSupportedContent"
      aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <i class="fas fa-bars"></i>
    </button>

    <!-- Collapsible wrapper -->
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <!-- Navbar brand -->
      <a class="navbar-brand mt-2 mt-lg-0" href="/admin/pagAdmin">
        <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/3/34/EY_logo_2019.svg/640px-EY_logo_2019.svg.png"
          height="45" alt="MDB Logo" loading="lazy" style="padding-bottom: 10px;" />
      </a>

    </div>

    <a style="color: #fff" class="text-reset me-4 text-white" href="/admin/listaUsuarios">
      <i id="estrelaico" class="fas fa-scroll"></i>
    </a>

    <a style="color: #fff" class="text-reset me-4 text-white" href="homevaga.html">
      <i id="estrelaico" class="fas fa-list-alt"></i>
    </a>

    <a style="color: #fff;" class="text-reset me-4 text-white" href="/logout">
            <p>Sair</p>
    </a>

    <!-- Avatar -->
    <div class="dropdown">
      <a class="dropdown-toggle d-flex align-items-center hidden-arrow" href="#" id="navbarDropdownMenuAvatar"
        role="button" data-mdb-toggle="dropdown" aria-expanded="false">
        <img
          src="https://cdn.create.vista.com/api/media/medium/471200328/stock-vector-big-cogwheel-gold-plated-metalic?token="
          class="rounded-circle" height="45" loading="lazy" />
      </a>

    </div>
  </div>
  <!-- Right elements -->
  </div>
  <!-- Container wrapper -->
</nav>
<!-- Navbar -->

{{else}}

